{
    "name": "jaytaph/spacetraders-cli",
    "description": "PHP console app for the SpaceTraders.io API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "spacetraders",
        "client"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joshua Thijssen",
            "email": "jthijssen@noxlogic.nl"
        }
    ],
    "require": {
        "jaytaph/spacetraders": "dev-main",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    }
}
